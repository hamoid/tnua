// this makes a window of the specified size
size(800, 400);

// this makes a full screen window
//fullScreen();

background(255, 255, 255);

// I can draw circles with this
// x, y, width, height
ellipse(random(width), random(height), 100, 100);
// set the inside color of shapes
fill(255, 200, 50);  // egg yellow
// set the border color of shapes
stroke(255, 100, 0);
// set the line thickness
strokeWeight(10);
// to draw lines
line(0, 0, 200, 200);
// to draw rectangle
fill(0, 200, 200);  // egg yellow
rect(200, 200, 100, 100);

fill(255, 0, 200);  // egg yellow
rect(400, 200, 100, 100);

fill(155, 200, 50);  // egg yellow
rect(600, 200, 100, 100);